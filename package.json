  "scripts": {
    "build": "npm run build-server && node scripts/build.js && npm run build-scss",
    "build-src-folder": " NODE_ENV=production ./node_modules/.bin/babel src -d build-server/src",
    "build-server-folder": " NODE_ENV=production ./node_modules/.bin/babel server -d build-server/server",
    "build-server": "mkdir -p build-server/src build-server/server && npm run build-server-folder && npm run build-src-folder",
    "build-scss": "mkdir -p build/static/css && ./node_modules/node-sass/bin/node-sass ./src/components/bundle.scss --output-style compressed build/static/css/bundle.css",
    "dev-src": "node scripts/start.js",
    "dev-server": "mkdir -p build-server/src build-server/server build-server/bundle && concurrently  \"npm run dev-src-folder\" \"npm run dev-server-folder\" \"npm run dev-bundle\" \"npm run dev-scss\" \"NODE_ENV=development nodemon server.js\"",
    "dev-src-folder": " NODE_ENV=development ./node_modules/.bin/babel -w src -d build-server/src",
    "dev-server-folder": " NODE_ENV=development ./node_modules/.bin/babel -w server -d build-server/server",
    "dev-bundle": "NODE_ENV=development PORT=3001 node scripts/start.js",
    "dev-scss:build": "./node_modules/node-sass/bin/node-sass ./src/components/bundle.scss -o build-server/bundle",
    "dev-scss:watch": "./node_modules/node-sass/bin/node-sass --watch ./src/components/bundle.scss -o build-server/bundle",
    "dev-scss": "npm run dev-scss:build && npm run dev-scss:watch",
    "nodemon": "./node_modules/nodemon/bin/nodemon.js",
    "postinstall": "npm run build",
    "start": "NODE_ENV=production node server.js",
    "test": "node scripts/test.js --env=jsdom"
  },
  "babel": {
    "presets": [
      "env",
      "react-app"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
      "import/no-named-as-default": 0,
      "import/no-named-as-default-member": 0,
      "no-underscore-dangle": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "jsx-a11y/label-has-for": 0
    },
    "env": {
      "browser": true,
      "node": true
    }
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-react": "^7.7.0",
    "http-proxy-middleware": "^0.18.0",
    "nodemon": "^1.12.1",
    "webpack-dev-server": "2.8.2"
  }
}
